{
  "name": "room.js",
  "version": "0.0.1",
  "author": "Chris Dos√© <chris.dose@gmail.com>",
  "description": "A MOO in CoffeeScript",
  "private": true,
  "repository": "https://github.com/doughsay/room.js",
  "scripts": {
    "dev": "node --harmony ./node_modules/.bin/coffee app/app.coffee",
    "start": "./node_modules/.bin/forever start --killSignal=SIGTERM -c \"node --harmony ./node_modules/.bin/coffee\" app/app.coffee",
    "stop": "./node_modules/.bin/forever stop app/app.coffee"
  },
  "dependencies": {
    "express": "*",
    "jade": "*",
    "coffee-script": "*",
    "socket.io": "*",
    "ff": "*",
    "underscore": "*",
    "cron": "*",
    "log4js": "*",
    "forever": "*"
  },
  "devDependencies": {
    "grunt": "*",
    "grunt-contrib-jshint": "*",
    "grunt-contrib-uglify": "*",
    "grunt-contrib-coffee": "*",
    "grunt-contrib-concat": "*",
    "grunt-contrib-less": "*",
    "grunt-contrib-clean": "*",
    "grunt-contrib-copy": "*",
    "grunt-contrib-watch": "*"
  },
  "engines": {
    "node": ">=0.11.3"
  }
}
